package co.uniquindio.prog2.biblioteca.model;

/**
 * Clase bibliotecario, extiende de clase persona
 * 
 * @author Juan Andrade
 *
 */
public class Bibliotecario extends Persona {

	// ATRIBUTOS CLASE
	private int antiguedad;

	public Bibliotecario(String nombre, String identificacion, String telefono, String direccion, String ocupacion,
			int antiguedad, String tipoDocumento) {
		super(nombre, identificacion, telefono, direccion, ocupacion);
		this.antiguedad = antiguedad;
		// TODO Auto-generated constructor stub
	}

	/**
	 * @return the antiguedad
	 */
	public int getAntiguedad() {
		return antiguedad;
	}

	/**
	 * @param antiguedad the antiguedad to set
	 */
	public void setAntiguedad(int antiguedad) {
		this.antiguedad = antiguedad;
	}

}

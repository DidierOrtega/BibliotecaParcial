package co.uniquindio.prog2.biblioteca.model;

public class DetallePrestamo {

	private String  codigo;
	private int cantidad;
	private Libro libro;
	private double cantidadSutTotal;


	public DetallePrestamo(int cantidad, Libro libro, double cantidadSutTotal,String codigo) {
		super();
		this.cantidad = cantidad;
		this.libro = libro;
		this.cantidadSutTotal=cantidadSutTotal;
		this.codigo=codigo;
	}


	/**
	 * @return the cantidadSutTotal
	 */
	public double getCantidadSutTotal() {
		return cantidadSutTotal;
	}


	/**
	 * @param cantidadSutTotal the cantidadSutTotal to set
	 */
	public void setCantidadSutTotal(double cantidadSutTotal) {
		this.cantidadSutTotal = cantidadSutTotal;
	}


	public int getCantidad() {
		return cantidad;
	}


	public void setCantidad(int cantidad) {
		this.cantidad = cantidad;
	}


	public Libro getLibro() {
		return libro;
	}


	public void setLibro(Libro libro) {
		this.libro = libro;
	}


	@Override
	public String toString() {
		return "DetallePrestado [cantidad=" + cantidad + ", libro=" + libro + "subTotal:"+ cantidadSutTotal+ "]";
	}

	//1.0 Consultar la cantidad de libros de la editorial “Uniquindio”
		//que han sido prestados  en la biblioteca y su título empiece por una vocal.
//	public int obtenerCantidadLibrosEditorialVocal(String editorial) {
//
//		int cantidad =0;
//		boolean verificado = false;
//
//		if(libro != null){
//			verificado = libro.verificarEditorialLetraVocal(editorial);
//			if(verificado == true){
//				cantidad = this.cantidad;
//			}
//		}
//		return cantidad;
//	}


}
